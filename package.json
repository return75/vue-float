{
  "name": "vue-float",
  "version": "0.1.2",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-float src/index.js",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/vue-float.common.js",
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/return75/vue-float.git"
  },
  "files": [  "dist/*",  "src/*",  "public/*",  "*.json",  "*.js"],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
